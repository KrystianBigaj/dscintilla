package DScintillaRuntime;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'DScintilla runtime package'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  rtl,
{$IFNDEF UNICODE}
  Jcl,
{$ENDIF}
  vcl;

contains
  DScintillaTypes in '..\Sources\DScintillaTypes.pas',
  DScintillaUtils in '..\Sources\DScintillaUtils.pas',
  DScintillaCustom in '..\Sources\DScintillaCustom.pas',
  DScintilla in '..\Sources\DScintilla.pas';

end.
